<?xml version="1.0" encoding="UTF-8"?>
<Configuration status="warn" packages="service-flow.assignment.log">
    <Appenders>
        <Console name="StandardOut" target="SYSTEM_OUT">
            <PatternLayout>
                <Pattern>%d{HH:mm:ss.SSS} [%t] %-5level %logger{36} - %msg%n</Pattern>
            </PatternLayout>
        </Console>
    </Appenders>
    <Loggers>
        <Root level="debug">
            <AppenderRef ref="StandardOut" level="debug"/>
        </Root>
    </Loggers>
</Configuration>
